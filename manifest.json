{
    "name": "MMMUT Wifi Login", 
    "manifest_version": 3,
    "version": "1.0.0",
    "description": "This Extension will sign you in into the MMMUT wifi",
    "permissions": ["tabs","scripting"],
    "action": {   
      "default_popup": "pop.html", 
      "default_icon": {
        "16" : "Images/16.png",
        "32" : "Images/32.png",
        "48" : "Images/48.png",
        "128" : "Images/128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "content_scripts": [
      {
        "matches":["http://172.16.1.3:8090/*"],
        "css":["contentstyle.css"],
        "js":["contentScripts.js"]
      }
    ],
    "content_security_policy": {
    "extension_pages": "default-src 'self'; style-src 'self' 'unsafe-inline'; script-src 'self'"
 }
  } 